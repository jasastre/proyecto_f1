---
import $ from 'jquery';
import Layout from "../layouts/Layout.astro";
import LandingHeader from "../components/LandingHeader.astro";
import TrackTimeSection from '../components/TrackTimeSection.astro';
import { getSession } from "auth-astro/server";

const session = await getSession(Astro.request);
console.log(session);

---

<Layout title="F1® España">
	<LandingHeader />

	<main class="snap-y snap-mandatory relative w-full h-screen overflow-auto">
		<div class="snap-center">
			{
				session?(
					<TrackTimeSection />
				):(
					<script>
						window.location.href = '/login';
					</script>
				)
			}
		</div>
	</main>
</Layout>
